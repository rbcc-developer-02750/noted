    {% endif %}
    "sameAs": [
      {{ settings.social_twitter_link | json }},
      {{ settings.social_facebook_link | json }},
      {{ settings.social_pinterest_link | json }},
      {{ settings.social_instagram_link | json }},
      {{ settings.social_tiktok_link | json }},
      {{ settings.social_tumblr_link | json }},
      {{ settings.social_snapchat_link | json }},
      {{ settings.social_youtube_link | json }},
      {{ settings.social_vimeo_link | json }}
    ],
    "url": {{ request.origin | append: page.url | json }}
  }
</script>

<script>
  function handleScroll() {
      const stickyNav = document.querySelector('sticky-header');
      const links = document.querySelectorAll('.header__menu-item');
      const cart = document.querySelector('#cart-icon-bubble');
      const mobileMenu = document.querySelector('.header__icon--menu');
    
      if (window.scrollY === 0) {
        stickyNav.style.background = 'transparent'
        stickyNav.style.borderBottom = 'none'
        mobileMenu.style.color = '#fff'
        links.forEach(link => {
          link.style.color = '#fff';
        })
        cart.style.color = '#fff';
      } else {
        stickyNav.style.background = 'white'
        stickyNav.style.borderBottom = '.1rem solid rgba(var(--color-foreground), .08) !important'
        mobileMenu.style.color = '#000'
        links.forEach(link => {
          link.style.color = '#000';
        })
        cart.style.color = '#000';
      }
  }
  
  window.addEventListener('scroll', handleScroll);
</script>

{%- if request.page_type == 'index' -%}
  {% assign potential_action_target = request.origin | append: routes.search_url | append: "?q={search_term_string}" %}
  <script type="application/ld+json">
    {
      "@context": "http://schema.org",
      "@type": "WebSite",
      "name": {{ shop.name | json }},
      "potentialAction": {
        "@type": "SearchAction",
        "target": {{ potential_action_target | json }},
        "query-input": "required name=search_term_string"
      },
      "url": {{ request.origin | append: page.url | json }}
    }
  </script>
{%- endif -%}

